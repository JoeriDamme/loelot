  "name": "loelot",
  "version": "1.0.0",
  "description": "loelot project",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "test": "cross-env NODE_ENV=test nyc --require ts-node/register mocha './test/**/*.spec.ts'",
    "test:unit": "cross-env NODE_ENV=test nyc --require ts-node/register mocha './test/unit/**/*.spec.ts'",
    "test:integration": "cross-env NODE_ENV=test nyc --require ts-node/register mocha './test/integration/**/*.spec.ts'",
    "dev": "nodemon",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "sync": "cross-env NODE_ENV=production ts-node script/sync.ts"
  },
  "pre-commit": [
    "lint",
    "test:unit",
    "test:integration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoeriDamme/loelot.git"
